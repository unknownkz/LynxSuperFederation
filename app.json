{
      "name": "LynxSuperFederation",
      "description": "A robot that manages a group that puts the federation first.",
      "keywords": [
         "telegram",
         "group",
         "manager",
         "logs"
      ],

   "repository": "https://github.com/unknownkz/LynxSuperFederation"
   "telegram": "https://t.me/xelyourslurred",
   "addons": [
          {
         "options": {
            "version": "12"
         },
         "plan": "heroku-postgresql"
      }
   ],
   "buildpacks": [
    {
      "url": "https://github.com/heroku/heroku-buildpack-python"
    },
    {
      "url": "https://github.com/jonathanong/heroku-buildpack-ffmpeg-latest"
    }
   ],
    "env": {
       "API_ID": {
         "description": "Get API_ID from my.telegram.org",
         "required": true  
       },
       "API_HASH": {
         "description": "Get API_HASH from my.telegram.org",
         "required": true
       },    
       "LOGS": {
         "description": "Fill ur Channel ID in here",
         "value": "-100"
         "required": true
       },
       "EVENT_LOGS": {
         "description": "Event Logs Channel to note down important bot level events, recommend to make this public. ex: '-100'",
         "required": true,
         "value": "-100"
       },
       "ERROR_LOGS": {
         "description": "Error Logs Channel to note down important bot errors events, make this public/private. ex: '-100'",
         "required": true,
         "value": "-100"
       },
       "BOT_TOKEN": {
         "description": "Take the token at @BotFather",
         "required": false
       },
       "BOT_USERNAME": {
         "description": "Fill ur bot Username which from @BotFather",
         "required": true
       },
       "HEROKU_APP_NAME": {
         "description": "Name Of App heroku.",
         "required": true
       },
       "HEROKU_API_KEY": {
         "description": "Your HEROKU Api Key, get from 'https://dashboard.heroku.com/account'",
         "value": "Check On Your Heroku Dashboard",
         "required": true
       },
       "MONGO_DB_URI": {
         "description": "Required for database connections",
         "required": true,
         "value": ""
       },
       "SQLALCHEMY_DATABASE_URI": {
         "description": "Your postgres sql db, empty this field if you dont have one",
         "required": false,
         "value": "sqldbtype://username:pw@hostname:port/db_name"
       },      
       "ARQ_API": {
         "description": "Get this from @ARQRobot",
         "required": true
       },
       "SPAMWATCH_API": {
         "description": "Spamwatch API Token, Get one from @SpamWatchBot",
         "required": false,
         "value": ""
       }, 
       "URL": {
         "description": "The Heroku App URL -->  https://<appname>.herokuapp.com/",
         "required": true,
         "value": "https://"
       }
    }
}
